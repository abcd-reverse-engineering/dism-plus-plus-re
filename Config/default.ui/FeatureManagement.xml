<?xml version="1.0" encoding="utf-8"?>
<Root>
  <DefaultControl name="功能信息">
    <TreeNode height="22" inset="7,0,0,0" itemattr="valign='vcenter' font='0'" folderattr="width='10' enabled='false' selected='true' padding='4,3,5,0' align='left' disabledimage=&quot;file='Icon/list_icon_Disable.png' dest='0,2,9,11'&quot; selectedimage=&quot;file='Icon/list_icon_a.png' dest='0,2,9,11'&quot; normalimage=&quot;file='Icon/list_icon_b.png' dest='0,2,9,11'&quot; font='0'" checkboxattr="inset='20,0,0,0' textcolor=&quot;#FFbac0c5&quot;  disabledtextcolor=&quot;#FFbac0c5&quot; textpadding=&quot;20,2,0,0&quot; align=&quot;left&quot; selectedimage=&quot;file='Icon/PartChecked.png' dest='0,2,16,18'|file='Icon/Checked.png' dest='0,2,16,18'&quot; normalimage=&quot;file='Icon/unchecked.png' dest='0,2,16,18'&quot; font=&quot;0&quot;"/>
  </DefaultControl>
  <DefaultControl name="功能信息Win8">
    <TreeNode height="22" inset="7,0,0,0" itemattr="valign='vcenter' font='0'" folderattr="width='10' enabled='false' selected='true' padding='4,3,5,0' align='left' disabledimage=&quot;file='Icon/list_icon_Disable.png' dest='0,2,9,11'&quot; selectedimage=&quot;file='Icon/list_icon_a.png' dest='0,2,9,11'&quot; normalimage=&quot;file='Icon/list_icon_b.png' dest='0,2,9,11'&quot; font='0'" checkboxattr="inset='20,0,0,0' textcolor=&quot;#FFbac0c5&quot;  disabledtextcolor=&quot;#FFbac0c5&quot; textpadding=&quot;20,2,0,0&quot; align=&quot;left&quot; selectedimage=&quot;file='Icon/PartChecked.png' dest='0,2,16,18'|file='Icon/Checked.png' dest='0,2,16,18'|file='Icon/Delete.png' dest='0,2,16,18'&quot; normalimage=&quot;file='Icon/unchecked.png' dest='0,2,16,18'&quot; font=&quot;0&quot;"/>
  </DefaultControl>

  <DefaultControl name="应用程序">
    <ListContainerElement height="40" inset="5,5,5,5">
      <HorizontalLayout>
        <Label name="名称" font="0"/>
        <Label name="安装日期" font="0" width="120" align="center"/>
        <Label name="大小" font="0" width="120" align="center"/>
      </HorizontalLayout>
    </ListContainerElement>
  </DefaultControl>
  <VerticalLayout>
    <HorizontalLayout height="50" name="AppxOption">
      <Option text="#应用程序" width="200"  textpadding="5,0,0,0" font="1" group="AppxGroup" selectedtextcolor="#FF007ACC" selected="true"/>
      <Option text="#Windows功能" width="200"  textpadding="5,0,0,0" font="1" group="AppxGroup" selectedtextcolor="#FF007ACC"/>
    </HorizontalLayout>
    <TabLayout name="StartTab" selectedid="0">
      <!--应用程序-->
      <VerticalLayout>
        <List name="应用列表" vscrollbar="true" childpadding="3,3,3,3" inset="0,0,0,3">
          <ListHeader bkcolor="#FF9BC716">
            <ListHeaderItem name="程序名称" text="#项目" font="0" textcolor="#FF1E1E1E"/>
            <ListHeaderItem name="安装日期" text="#安装日期" font="0" textcolor="#FF1E1E1E" width="120"/>
            <ListHeaderItem name="大小" text="#大小" font="0" textcolor="#FF1E1E1E" width="120"/>
          </ListHeader>
        </List>
        <HorizontalLayout height="40" inset="5,5,5,5" childpadding="5">
          <Button name="删除" text="#删除" width="80" font="0" bordercolor="#FF252526" visible="false" bordersize="1"/>
          <Button name="更改" text="#更改" width="80" font="0" bordercolor="#FF252526" visible="false" bordersize="1"/>
          <HorizontalLayout/>
          <Button name="添加" text="#添加" width="80" font="0" bordercolor="#FF252526" visible="false" bordersize="1"/>
          <Button name="刷新程序" text="#刷新" width="80" font="0" bkcolor="#FF007ACC" bordercolor="#FF252526" bordersize="1" textcolor="#FFFFFFFF"/>
        </HorizontalLayout>
      </VerticalLayout>
      
      <!--Windows功能-->
      <VerticalLayout>
        <VerticalLayout>
          <VerticalLayout>
            <HorizontalLayout padding="0,10,0,0">
              <TreeView name="功能列表" visiblefolderbtn="true" childpadding="4" inset="5,5,5,5" bordersize="1" bordercolor="#FF2B2B2B" vscrollbar="true" padding="10,0,10,0" itemcheckimgsize="20,20" visiblecheckbtn="true" worktype="2"/>
              <VerticalLayout name="本地源UI" width='300'>
                <Label text="#本地源：" font="0" padding="5,5,5,5"/>
                <HorizontalLayout>
                  <Edit width='200' name="本地源" cuebannertext="#请在此输入本地源" font="0" height="30" textpadding="5,5,5,5" bordercolor="#FF252526" bordersize="1" padding="10,0,0,0"/>
                  <Button name="本地源浏览" text="#浏览" width="80" height="30" font="0" bordercolor="#FF252526" bordersize="1" padding="5,0,0,0"/>
                </HorizontalLayout>
              </VerticalLayout>
            </HorizontalLayout>
          </VerticalLayout>
          <HorizontalLayout height="40" inset="5,5,5,5" childpadding="5">
            <Option text="#关闭功能" width="80" normalimage="file='Icon/unchecked.png' dest='0,6,16,22'" textpadding="20,5,0,0" font="0" disabledtextcolor="#FF000000" enabled="false" align="left"/>
            <Option text="#部分启用" width="80" normalimage="file='Icon/PartChecked.png' dest='0,6,16,22'" textpadding="20,5,0,0" font="0" disabledtextcolor="#FF000000" enabled="false" align="left"/>
            <Option text="#全部启用" width="80" normalimage="file='Icon/Checked.png' dest='0,6,16,22'" textpadding="20,5,0,0" font="0" disabledtextcolor="#FF000000" enabled="false" align="left"/>
            <Option text="#删除功能" width="80" normalimage="file='Icon/Delete.png' dest='0,6,16,22'" textpadding="20,5,0,0" font="0" disabledtextcolor="#FF000000" enabled="false" align="left"/>
            <HorizontalLayout/>
            <Button name="刷新" text="#刷新" width="80" font="0" bordercolor="#FF252526" bordersize="1"/>
            <Button name="应用" text="#应用" width="80" font="0" bkcolor="#FF007ACC" bordercolor="#FF252526" bordersize="1" textcolor="#FFFFFFFF"/>
          </HorizontalLayout>
        </VerticalLayout>
      </VerticalLayout>
    </TabLayout>
  </VerticalLayout>
</Root>